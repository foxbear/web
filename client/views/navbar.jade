.navbar-wrapper
  .container
    .navbar.navbar-default.navbar-static-top(role='navigation')
      .container
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='/') #{general.name}
        .navbar-collapse.collapse
          ul.nav.navbar-nav
            li.active
              a(href='/') Home
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                | Services 
                span.caret
              ul.dropdown-menu(role='menu') 
                each service in general.services
                  li
                    a(href='./#'+service.name) #{service.heading}
            li.dropdown
              a.dropdown-toggle(href='#', data-toggle='dropdown')
                | Projects 
                span.caret
              ul.dropdown-menu(role='menu')
                each category in projects
                  li.divider
                  li.dropdown-header #{category.heading}
                    each project in category.projects
                      li
                        a(href='./projects#'+project.name) #{project.heading}
            li
              a(href='/about') About
            li
              a(href='/contact') Contact